{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "hours_video": {
      "type": "decimal",
      "default": 0
    },
    "number_of_lectures": {
      "type": "integer"
    },
    "rating": {
      "type": "decimal"
    },
    "url": {
      "type": "string"
    },
    "authors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::author.author",
      "inversedBy": "courses"
    }
  }
}
